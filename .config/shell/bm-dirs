cac ${XDG_CACHE_HOME:-$HOME/.cache}         # cache
cf  ${XDG_CONFIG_HOME:-$HOME/.config}       # config
D   ${XDG_DOWNLOAD_DIR:-$HOME/Downloads}    # download
d   ${XDG_DOCUMENTS_DIR:-$HOME/Documents}   # documents
dt  ${XDG_DATA_HOME:-$HOME/.local/share}    # share
h   $HOME                                   # home
m   ${XDG_MUSIC_DIR:-$HOME/Music}           # music
mnt /mnt                                    # mnt
pp  ${XDG_PICTURES_DIR:-$HOME/Pictures}     # pictures
sc  $HOME/.local/bin                        # executables
src $HOME/.local/src                        # builds
vv  ${XDG_VIDEOS_DIR:-$HOME/Videos}         # videos
usb /run/media/$USER                        # usb
